<p style="color: green"><%= notice %></p>

<h1>Home</h1>
<%= button_to 'Scrape', scrape_podcastscrappings_path %>
<%= link_to "Create user", user_index_path %>
<%= link_to "Login", user_login_path %>

<div id="podcastscrappings">
  <% @podcastscrappings.each do |podcastscrapping| %>
    <%= render podcastscrapping %>
    <p>
      <%= link_to "Show this podcast", podcastscrapping %>
    </p>
  <% end %>
</div>
