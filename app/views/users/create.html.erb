<%form_for @user, controller: 'users', action: 'create' do |f|%>
    <%= f.label :name%>
    <%= f.text_field :name%>

    <%= f.label :password%>
    <%= f.password :password%>

    <%= f.label :password_confirmation%>
    <%= f.password :password_confirmation%>

    <%= f.submit "create"%>
<%end%>