<%= form_with url: create_user_path , method: :get do |f| %>
    <%= f.label :name%>
    <%= f.text_field :name%>

    <%= f.label :email%>
    <%= f.text_field :email%>

    <%= f.label :password%>
    <%= f.password_field :password_digest%>

    <%= f.label :password_confirmation%>
    <%= f.password_field :password_confirmation%>

    <%= f.submit "create"%>
<% end %>